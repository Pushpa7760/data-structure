#include<stdio.h>
#define SIZE 6
int queue[SIZE];
int front =-1,rear=-1;
void enqueue(int value){
    if(rear==SIZE - 1){
        printf("queue is full");
    }
    else{
        if(front=-1)
        {
            front=0;
        }
        rear++;
        queue[rear]=value;
        printf("%d ",value);
    }
}
void dequeue()
{
    if(front==-1){
        printf("empty");
    }
    else{
        printf("\ndeleted %d\n",queue[front]);
        front ++;
    }
}
void peek()
{
    if(front==-1||front>rear){
        printf("empty");
    }
    else{
        printf("first elements is %d",queue[front]);
    }
}
int main()
{
    enqueue(10);
    enqueue(20);
    dequeue();
    peek();
    return 0;
}
